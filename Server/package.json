{
    "name": "tech-scribe-backend",
    "version": "1.0.0",
    "main": "src/server.js",
    "repository": "https://github.com/Magic-Touch-Laundry/Service-website-backend.git",
    "author": "nikhilenigma <nickharmalkar18@gmail.com>",
    "license": "MIT",
    "lint-staged": {
        "*.ts": [
            "yarn lint:fix",
            "yarn format:fix"
        ]
    },
    "scripts": {
        "prepare": "husky",
        "dist": "npx tsc",
        "dev": "cross-env NODE_NO_WARNINGS=1 NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
        "start": "cross-env NODE_ENV=production node dist/server.js",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format:check": "prettier . --check",
        "format:fix": "prettier . --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "typecheck": "tsc --noEmit",
        "db:seed": "prisma db seed"
    },
    "_moduleAliases": {
        "@": "dist/src",
        "@features": "dist/src/features",
        "@config": "dist/src/config",
        "@utils": "dist/src/utils",
        "@types": "dist/src/types",
        "@middlewares": "dist/src/api/middlewares"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@eslint/js": "^9.11.1",
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/jexl": "^2.3.4",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.6.1",
        "@types/nodemailer": "^6.4.17",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^10.0.1",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.10",
        "prettier": "3.5.0",
        "prisma": "^6.2.1",
        "ts-jest": "^29.3.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.7.3",
        "typescript-eslint": "^8.7.0"
    },
    "dependencies": {
        "@prisma/client": "^6.2.1",
        "ajv": "^8.17.1",
        "argon2": "^0.41.1",
        "axios": "^1.7.9",
        "bcrypt": "^5.1.1",
        "cloudinary": "^2.5.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv-flow": "^4.1.0",
        "express": "^4.21.0",
        "helmet": "^8.0.0",
        "jexl": "^2.3.0",
        "jsonwebtoken": "^9.0.2",
        "module-alias": "^2.2.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.16",
        "openai": "^4.83.0",
        "punycode": "^2.3.1",
        "resend": "^4.0.1",
        "winston": "^3.14.2",
        "zod": "^3.23.8"
    },
    "prisma": {
        "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
    }
}
